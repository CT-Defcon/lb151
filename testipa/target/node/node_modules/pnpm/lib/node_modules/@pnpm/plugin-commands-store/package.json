{
	"name": "@pnpm/plugin-commands-store",
	"version": "2.0.40",
	"description": "Commands for controlling the store",
	"main": "lib/index.js",
	"typings": "lib/index.d.ts",
	"files": [
		"lib",
		"!*.map"
	],
	"engines": {
		"node": ">=10.13"
	},
	"scripts": {
		"lint": "eslint -c ../../eslint.json src/**/*.ts test/**/*.ts",
		"registry-mock": "registry-mock",
		"test:tap": "cd ../.. && c8 --reporter lcov --reports-dir packages/plugin-commands-store/coverage ts-node packages/plugin-commands-store/test --type-check",
		"test:e2e": "registry-mock prepare && run-p -r registry-mock test:tap",
		"_test": "cross-env PNPM_REGISTRY_MOCK_PORT=7779 pnpm run test:e2e",
		"test": "pnpm run compile && pnpm run _test",
		"prepublishOnly": "pnpm run compile",
		"compile": "rimraf lib tsconfig.tsbuildinfo && tsc --build"
	},
	"repository": "https://github.com/pnpm/pnpm/blob/master/packages/plugin-commands-store",
	"keywords": [
		"pnpm",
		"scripts"
	],
	"author": "Zoltan Kochan <z@kochan.io> (https://www.kochan.io/)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/pnpm/pnpm/issues"
	},
	"homepage": "https://github.com/pnpm/pnpm/blob/master/packages/plugin-commands-store#readme",
	"devDependencies": {
		"@pnpm/assert-store": "1.0.10",
		"@pnpm/lockfile-file": "3.0.14",
		"@pnpm/logger": "^3.2.2",
		"@pnpm/plugin-commands-store": "link:",
		"@pnpm/prepare": "0.0.11",
		"@types/archy": "0.0.31",
		"@types/ramda": "^0.27.15",
		"@types/sinon": "^9.0.5",
		"@types/ssri": "^6.0.3",
		"@zkochan/rimraf": "^1.0.0",
		"execa": "^4.0.3",
		"load-json-file": "^6.2.0",
		"path-exists": "^4.0.0",
		"sinon": "^9.0.3",
		"ssri": "6.0.1",
		"tempy": "^0.7.0"
	},
	"dependencies": {
		"@pnpm/cafs": "^2.0.0",
		"@pnpm/cli-utils": "0.4.28",
		"@pnpm/config": "11.3.0",
		"@pnpm/error": "1.3.1",
		"@pnpm/get-context": "3.2.6",
		"@pnpm/lockfile-utils": "2.0.16",
		"@pnpm/normalize-registries": "1.0.3",
		"@pnpm/parse-wanted-dependency": "1.0.0",
		"@pnpm/pick-registry-for-package": "1.0.3",
		"@pnpm/pkgid-to-filename": "^3.0.0",
		"@pnpm/store-connection-manager": "0.3.35",
		"@pnpm/store-controller-types": "9.1.0",
		"@pnpm/store-path": "^4.0.2",
		"@pnpm/types": "6.2.0",
		"archy": "^1.0.0",
		"dependency-path": "5.0.3",
		"dint": "^5.0.0",
		"p-filter": "^2.1.0",
		"ramda": "^0.27.1",
		"render-help": "^1.0.0"
	},
	"peerDependencies": {
		"@pnpm/logger": "^3.1.0"
	},
	"funding": "https://opencollective.com/pnpm"
}

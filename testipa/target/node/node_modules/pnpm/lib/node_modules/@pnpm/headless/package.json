{
	"name": "@pnpm/headless",
	"description": "Fast installation using only pnpm-lock.yaml",
	"version": "14.2.0",
	"author": "Zoltan Kochan <z@kochan.io> (https://www.kochan.io/)",
	"bugs": {
		"url": "https://github.com/pnpm/pnpm/issues"
	},
	"main": "lib/index.js",
	"typings": "lib/index.d.ts",
	"files": [
		"lib",
		"!*.map"
	],
	"peerDependencies": {
		"@pnpm/logger": "^3.1.0"
	},
	"devDependencies": {
		"@pnpm/assert-project": "2.1.9",
		"@pnpm/client": "^2.0.4",
		"@pnpm/headless": "link:",
		"@pnpm/logger": "^3.2.2",
		"@pnpm/package-store": "10.1.0",
		"@pnpm/read-projects-context": "4.0.5",
		"@pnpm/store-path": "^4.0.2",
		"@pnpm/test-fixtures": "^0.0.0",
		"@types/fs-extra": "^9.0.1",
		"@types/mz": "^2.7.1",
		"@types/ramda": "^0.27.15",
		"@types/rimraf": "^3.0.0",
		"@types/sinon": "^9.0.5",
		"@types/tape-promise": "^4.0.1",
		"@zkochan/rimraf": "^1.0.0",
		"is-windows": "^1.0.2",
		"isexe": "2.0.0",
		"load-json-file": "^6.2.0",
		"mz": "^2.7.0",
		"npm-run-all": "^4.1.5",
		"sinon": "^9.0.3",
		"tape-promise": "4.0.0",
		"tempy": "^0.7.0",
		"write-json-file": "4.0.0"
	},
	"directories": {
		"test": "test"
	},
	"homepage": "https://github.com/pnpm/pnpm/blob/master/packages/headless#readme",
	"keywords": [
		"pnpm",
		"install",
		"installer"
	],
	"license": "MIT",
	"engines": {
		"node": ">=10.13"
	},
	"repository": "https://github.com/pnpm/pnpm/blob/master/packages/headless",
	"scripts": {
		"start": "pnpm run tsc -- --watch",
		"commit": "commit",
		"commitmsg": "commitlint -e",
		"lint": "eslint -c ../../eslint.json src/**/*.ts test/**/*.ts",
		"registry-mock": "registry-mock",
		"test:tap": "cd ../.. && c8 --reporter lcov --reports-dir packages/headless/coverage ts-node packages/headless/test --type-check",
		"test:e2e": "registry-mock prepare && run-p -r registry-mock test:tap",
		"pre_test": "ts-node test/pretest",
		"_test": "cross-env PNPM_REGISTRY_MOCK_PORT=7770 pnpm run test:e2e",
		"test": "pnpm run compile && pnpm run _test",
		"prepublishOnly": "pnpm run compile",
		"runPrepareFixtures": "node ../pnpm/lib/bin/pnpm.js i -r -C test/fixtures --no-shared-workspace-lockfile --no-link-workspace-packages --lockfile-only --registry http://localhost:4873/ --ignore-scripts --force",
		"prepareFixtures": "registry-mock prepare && run-p -r registry-mock runPrepareFixtures",
		"compile": "rimraf lib tsconfig.tsbuildinfo && tsc --build"
	},
	"dependencies": {
		"@pnpm/build-modules": "5.0.16",
		"@pnpm/constants": "4.0.0",
		"@pnpm/core-loggers": "5.0.0",
		"@pnpm/error": "1.3.1",
		"@pnpm/filter-lockfile": "4.0.9",
		"@pnpm/hoist": "4.0.12",
		"@pnpm/lifecycle": "9.2.5",
		"@pnpm/link-bins": "5.3.13",
		"@pnpm/lockfile-file": "3.0.14",
		"@pnpm/lockfile-utils": "2.0.16",
		"@pnpm/modules-cleaner": "10.0.11",
		"@pnpm/modules-yaml": "8.0.2",
		"@pnpm/package-requester": "12.1.0",
		"@pnpm/pkgid-to-filename": "^3.0.0",
		"@pnpm/read-package-json": "3.1.5",
		"@pnpm/read-project-manifest": "1.0.13",
		"@pnpm/store-controller-types": "9.1.0",
		"@pnpm/symlink-dependency": "3.0.10",
		"@pnpm/types": "6.2.0",
		"dependency-path": "5.0.3",
		"p-limit": "^3.0.2",
		"path-absolute": "^1.0.1",
		"path-exists": "^4.0.0",
		"ramda": "^0.27.1",
		"realpath-missing": "^1.0.0"
	},
	"funding": "https://opencollective.com/pnpm"
}
